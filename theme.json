{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [ "#7adcb4", "#0b8bc1" ],
					"slug": "green-and-blue",
					"name": "Green and Blue"
				}
			],
			"gradients": [
				{
					"slug": "diagonal-green-to-blue",
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--green) 0%,var(--wp--preset--color--blue) 100%)",
					"name": "Diagonal green to blue"
				}
			],
			"palette": [
				{
					"slug": "blue",
					"color": "#0b8bc1",
					"name": "Blue"
				},
				{
					"slug": "green",
					"color": "#7adcb4",
					"name": "Green"
				},
				{
					"slug": "grey-medium",
					"color": "#8F8F8F",
					"name": "Grey medium"
				},
				{
					"slug": "grey-light",
					"color": "#cccccc",
					"name": "Grey light"
				},
				{
					"slug": "grey-dark",
					"color": "#212121",
					"name": "Grey dark"
				},
				{
					"slug": "grey-darkest",
					"color": "#111111",
					"name": "Grey darkest"
				}
			]
		},
		"custom": {
			"spacing": {
				"tiny": "1vw",
				"small": "4vw",
				"medium": "8vw",
				"large": "16vw"
			},
			"typography": {
				"font-size": {
					"gigantic": "clamp(3rem, 6vw, 4rem)",
					"colossal": "clamp(4rem, 8vw, 6.25rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.5
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Inter\", serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "clamp(1.125rem, 3vw, 1.3rem)",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "1.75rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "clamp(2.5rem, 4vw, 3rem)",
					"slug": "huge"
				}
			]
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1000px"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--green)",
					"text": "var(--wp--preset--color--grey-dark)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--typography--font-size--normal)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/quote": {
				"border": {
					"width": "0 0 0 1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/image": {
				"border": {
					"radius": "12px"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--grey-darkest)",
			"text": "var(--wp--preset--color--grey-medium)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				},
				"color": {
					"text": "var(--wp--preset--color--grey-light)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)"
				},
				"color": {
					"text": "var(--wp--preset--color--grey-light)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"color": {
					"text": "var(--wp--preset--color--grey-light)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"color": {
					"text": "var(--wp--preset--color--grey-light)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"color": {
					"text": "var(--wp--preset--color--grey-light)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "Inter var, sans-serif",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--normal)"
				},
				"color": {
					"text": "var(--wp--preset--color--grey-light)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--grey-medium)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "Inter var, sans-serif",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
